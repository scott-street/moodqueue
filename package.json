{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "test:watch": "jest -watch",
    "test:coverage": "jest --collectCoverage",
    "cy:open": "cypress open"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/jest.tsconfig.json"
      }
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@types/styled-components": "^5.1.3",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "grommet": "^2.15.0",
    "grommet-icons": "^4.4.0",
    "next": "9.4.4",
    "notistack": "^1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "^16.13.1",
    "react-spinners": "^0.9.0",
    "styled-components": "^5.1.1",
    "styled-icons": "^10.19.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.6",
    "@types/react": "^16.9.43",
    "cypress": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
}
